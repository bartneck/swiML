<program>

    <!-- test program for python reading and writing-->
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program in the sun. The target duration was 60 minutes.</programDescription>
    <creationDate>2023-02-07</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
 
    
    
    <instruction>
        <continue>
            <totalLength>800</totalLength>
            <instruction>
                <lengthAsDistance updated="yes">100</lengthAsDistance>
                <stroke><kicking><legMovement>flutter</legMovement></kicking></stroke>
            </instruction>
            
            <instruction>
                <lengthAsDistance updated="yes">100</lengthAsDistance>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance updated="yes">100</lengthAsDistance>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>5</repetitionCount>
                    <instruction>
                        <lengthAsDistance>100</lengthAsDistance>
                        <rest><sinceStart>PT1M45S</sinceStart></rest>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                        <breath>5</breath>
                        <intensity><staticIntensity>
                            <zone>endurance</zone>
                        </staticIntensity></intensity>
                    </instruction>
                </repetition>
            </instruction>
        </continue>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><sinceStart>PT1M45S</sinceStart></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <breath>5</breath>
                <instructionDescription>do it faster then me</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
</program>
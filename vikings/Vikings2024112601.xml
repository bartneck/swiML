<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Vikings</title>
    <author>
        <firstName>Callum</firstName>
        <lastName>Lockhart</lastName>
        <email>clo78@uclive.ac.nz</email>
    </author>
    <programDescription>Tuesday Morning Training</programDescription>
    <creationDate>2024-10-29</creationDate>
    
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>


    <instruction>
        <repetition>
            <simplify>true</simplify>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT3M0S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>        
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillStroke>any</drillStroke>
                            </drill>
                        </stroke>
                        <rest>
                            <sinceStart>PT3M10S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>        
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
                <rest>
                    <sinceStart>PT4M0S</sinceStart>
                </rest>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <continue>
                            <continueLength>
                                <lengthAsDistance>200</lengthAsDistance>
                            </continueLength>
                            <stroke>
                                <standardStroke>any</standardStroke>
                            </stroke>
                            <rest>
                                <sinceStart>PT3M0S</sinceStart>
                            </rest>
                            <instruction>
                                <length>
                                    <lengthAsDistance>100</lengthAsDistance>
                                </length>
                                <stroke>
                                    <drill>
                                        <drillStroke>any</drillStroke>
                                    </drill>
                                </stroke>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>100</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>any</standardStroke>
                                </stroke>
                            </instruction>
                        </continue>
                    </instruction>
                </repetition>        
            </instruction>
        </repetition>        
    </instruction>


    
    <instruction>
        <segmentName>Main Set</segmentName>
    </instruction>
    

    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT0M45S</sinceStart>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>90</percentageEffort>
                            </startIntensity>
                        </intensity>
                        <equipment>fins</equipment>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT1M0S</sinceStart>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>85</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT0M35S</sinceStart>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>100</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT3M30S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M45S</sinceStart>
                </rest>
                <underwater>true</underwater>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>

    

    <instruction>
        <segmentName>Warm Down</segmentName>
    </instruction>


    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
        <equipment>fins</equipment>
    </instruction>
</program>
<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Regular Program 4</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>A freestyle set with a main block of 100 meters with increasing intensity.</programDescription>
    <creationDate>2024-06-04</creationDate>
    <pool>
        <poolLength>25</poolLength>
        <lengthUnit>meters</lengthUnit>
    </pool>
    
    <hideIntro>false</hideIntro>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <intensity>
                <startIntensity>
                    <percentageEffort>60</percentageEffort>
                </startIntensity>
                <stopIntensity>
                    <percentageEffort>90</percentageEffort>
                </stopIntensity>
            </intensity>
            <instruction>
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT1M35S</sinceStart>
                        </rest>
                        <instructionDescription>Extra 1:00 rest</instructionDescription>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>6</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>notFreestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
</program>

<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.0/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning program. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2024-03-17</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <drill>
                <drillName>any</drillName>
                <drillStroke>individualMedley</drillStroke>
            </drill>
        </stroke>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <kicking>
                <standardKick>freestyle</standardKick>
            </kicking>
        </stroke>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>6</repetitionCount>
            <intensity>
                <startIntensity>
                    <zone>easy</zone>
                </startIntensity>
                <stopIntensity>
                    <zone>racePace</zone>
                </stopIntensity>
            </intensity>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <equipment>fins</equipment>
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>butterfly</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>75</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>breaststroke</standardStroke>
        </stroke>
        <instructionDescription>butterfly kick</instructionDescription>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <drill>
                <drillName>any</drillName>
                <drillStroke>individualMedley</drillStroke>
            </drill>
        </stroke>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <equipment>pads</equipment>
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>backstroke</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>breaststroke</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>300</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>individualMedley</standardStroke>
        </stroke>
    </instruction>
</program>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
            
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
</program>
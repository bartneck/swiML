<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/ https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Triangular Numbers</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Swim the first 8 terms of the triangular numbers sequence.</programDescription>
    <creationDate>2024-04-22</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction><segmentName>Warm Up</segmentName></instruction>
    <instruction><length><lengthAsDistance>200</lengthAsDistance></length><stroke><standardStroke>any</standardStroke></stroke><intensity><startIntensity>
                <zone>easy</zone>
            </startIntensity></intensity></instruction>
    <instruction><segmentName>Triangular set</segmentName></instruction>

    <instruction><continue>
            <continueLength>
                <lengthAsLaps>1</lengthAsLaps>
            </continueLength>
            <instruction><length><lengthAsLaps>0</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke></instruction>
            <instruction><length><lengthAsLaps>1</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke></instruction>
        </continue></instruction>
    <instruction>
        <continue>
            <continueLength>
                <lengthAsLaps>3</lengthAsLaps>
            </continueLength>
            <instruction><length><lengthAsLaps>1</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke></instruction>
            <instruction><length><lengthAsLaps>2</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke></instruction>
        </continue>
    </instruction>
</program>

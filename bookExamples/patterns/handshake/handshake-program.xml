<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.1 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.1/swiML.xsd">
    <title>Triangular Numbers</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Swim the first 8 terms of the triangular numbers sequence.</programDescription>
    <creationDate>2024-04-22</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction><segmentName>Warm Up</segmentName></instruction>
    <instruction><length><lengthAsDistance>200</lengthAsDistance></length><stroke><standardStroke>any</standardStroke></stroke><intensity><startIntensity>
                <zone>easy</zone>
            </startIntensity></intensity></instruction>
    <instruction><segmentName>Triangular set</segmentName></instruction>
    <instruction><continue>
            <continueLength>1</continueLength>
            <instruction><length><lengthAsLaps>0</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>1</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>3</continueLength>
            <instruction><length><lengthAsLaps>1</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>2</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>6</continueLength>
            <instruction><length><lengthAsLaps>2</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>4</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>10</continueLength>
            <instruction><length><lengthAsLaps>3</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>7</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>15</continueLength>
            <instruction><length><lengthAsLaps>4</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>11</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>21</continueLength>
            <instruction><length><lengthAsLaps>5</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>16</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>28</continueLength>
            <instruction><length><lengthAsLaps>6</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>22</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><continue>
            <continueLength>36</continueLength>
            <instruction><length><lengthAsLaps>7</lengthAsLaps></length><stroke><standardStroke>notFreestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
            <instruction><length><lengthAsLaps>29</lengthAsLaps></length><stroke><standardStroke>freestyle</standardStroke></stroke><rest><afterStop>PT0M15S</afterStop></rest></instruction>
        </continue></instruction>
    <instruction><segmentName>Warm down</segmentName></instruction>
    <instruction><length><lengthAsDistance>200</lengthAsDistance></length><stroke><standardStroke>any</standardStroke></stroke><intensity><startIntensity>
                <zone>easy</zone>
            </startIntensity></intensity></instruction>
</program>

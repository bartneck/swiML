        <length>
            <lengthAsLaps>3</lengthAsLaps>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M15S</afterStop>
        </rest>
        <intensity>
            <startIntensity>
                <zone>racePace</zone>
            </startIntensity>
        </intensity>
    </instruction>

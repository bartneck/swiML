<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/latest https://raw.githubusercontent.com/bartneck/swiML/main/version/latest/swiML.xsd">
    <title>Polyomino</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Swimming the first four terms of the Polyomino sequence.</programDescription>
    <creationDate>2024-09-23</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>true</hideIntro>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <segmentName>Monomino</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT015S</afterStop>
        </rest>
    </instruction>

    <instruction>
        <segmentName>Domino</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT015S</afterStop>
        </rest>
    </instruction>

    <instruction>
        <segmentName>Tromino</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>300</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT015S</afterStop>
        </rest>
    </instruction>

    <instruction>
        <continue>

            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>

            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>



    <instruction>
        <segmentName>Tetromino</segmentName>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
    </instruction>

    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
</program>

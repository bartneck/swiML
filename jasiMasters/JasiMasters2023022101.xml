        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><kicking>
                <standardKick>notFreestyle</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>notFreestyle</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><dynamicAcross>false</dynamicAcross>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
            <stopIntensity>
                <zone>max</zone>
            </stopIntensity>
        </intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <instructionDescription>breath 25m as breath 3,5,7,3</instructionDescription>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <intensity><dynamicAcross>false</dynamicAcross>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
            <stopIntensity>
                <zone>max</zone>
            </stopIntensity>
        </intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
    </instruction>

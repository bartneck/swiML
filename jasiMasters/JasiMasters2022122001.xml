<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening session. The target duration was 60 minutes. The coach was Matt Nash. This was our last official training session of the year.</programDescription>
    <creationDate>2022-12-20</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
            <zone>easy</zone>
        </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking><standardKick>butterfly</standardKick></kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking><standardKick>backstroke</standardKick></kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>freestyle</drillStroke>
        </drill></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>individualMedley</drillStroke>
        </drill></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsTime>PT5S</lengthAsTime>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
                <underwater>true</underwater>
                <equipment>fins</equipment>
                <instructionDescription>wait at the wall</instructionDescription>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT45S</sinceStart></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <underwater>true</underwater>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsTime>PT5S</lengthAsTime>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
                <underwater>true</underwater>
                <equipment>fins</equipment>
                <instructionDescription>wait at the wall</instructionDescription>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT45S</sinceStart></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <breath>0</breath>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>fins</equipment>
        <instructionDescription>Fin on the right, pad on the left.</instructionDescription>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>fins</equipment>
        <instructionDescription>Fin on the left, pad on the right.</instructionDescription>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsTime>PT10S</lengthAsTime>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking><orientation>vertical</orientation>
                <legMovement>flutter</legMovement>
                </kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT45S</sinceStart></rest>
                <stroke><drill>
                    <drillName>technic</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsTime>PT10S</lengthAsTime>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking><orientation>vertical</orientation>
                    <legMovement>flutter</legMovement>
                </kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT45S</sinceStart></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>Relays</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <instructionDescription>Relay</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
                <instructionDescription>Relay</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
</program>
<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Tuesday evening program targeted at 60 minutes. The coach was Matt Nash</programDescription>
    <creationDate>2025-02-04</creationDate>

    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <layoutWidth>60</layoutWidth>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>butterfly</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>backstroke</drillStroke>
                    </drill>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M15S</afterStop>
        </rest>
        <breath>5</breath>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>breaststroke</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M15S</afterStop>
        </rest>
        <breath>4</breath>
    </instruction>


    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>butterfly</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M15S</afterStop>
        </rest>
        <breath>3</breath>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>breaststroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <segmentName>First set</segmentName>
    </instruction>

    <instruction>
        <continue>
            <equipment>fins</equipment>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>freestyle</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M45S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <continue>
            <equipment>pads</equipment>
            <equipment>pullBuoy</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                </intensity>
            </instruction>

            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>threshold</zone>
                    </startIntensity>
                </intensity>
            </instruction>

            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>endurance</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M45S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
                <rest>
                    <afterStop>PT0M0S</afterStop>
                </rest>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>notFreestyle</standardKick>
                    </kicking>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
                <rest>
                    <afterStop>PT0M0S</afterStop>
                </rest>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>notFreestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M45S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <segmentName>Second set</segmentName>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <repetition>
                    <simplify>true</simplify>
                    <rest>
                        <sinceStart>PT1M20S</sinceStart>
                    </rest>
                    <instruction>
                        <repetition>
                            <repetitionCount>1</repetitionCount>
                            <instruction>
                                <length>
                                    <lengthAsDistance>75</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>individualMedley</standardStroke>
                                </stroke>
                                <instructionDescription>Drop least favourite</instructionDescription>
                            </instruction>
                        </repetition>
                    </instruction>

                    <instruction>
                        <repetition>
                            <repetitionCount>2</repetitionCount>
                            <instruction>
                                <length>
                                    <lengthAsDistance>75</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                            </instruction>
                        </repetition>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>75</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>individualMedley</standardStroke>
        </stroke>
        <instructionDescription>Drop least favourite</instructionDescription>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedleyOrder</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT0M40S</sinceStart>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <zone>max</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
</program>

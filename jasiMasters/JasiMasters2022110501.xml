<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Saturday morning session in Kaikoura. The target duration was 90 minutes.</programDescription>
    <creationDate>2022-11-05</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>easy</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>easy</zone>
                    </staticIntensity></intensity>
                <stroke><kicking>
                        <standardKick>any</standardKick>
                    </kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>easy</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>any</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>6</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>notFreestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>300 </lengthAsDistance>
        <rest><afterStop>PT30S</afterStop></rest>
        <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>300 </lengthAsDistance>
        <rest><afterStop>PT30S</afterStop></rest>
        <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <lengthAsDistance>100</lengthAsDistance>
                        <rest><afterStop>PT20S</afterStop></rest>
                        <intensity><dynamicAcross>true</dynamicAcross>
                            <startIntensity><zone>easy</zone></startIntensity>
                            <stopIntensity><zone>racePace</zone></stopIntensity>
                        </intensity>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
            </instruction>
        </repetition>
    </instruction>
</program>

<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Sunday morning program targeted at 60 minutes. The coach was Matt Nash</programDescription>
    <creationDate>2024-03-26</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <layoutWidth>60</layoutWidth>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <repetition>
            <simplify>true</simplify>
            <rest>
                <afterStop>PT0M20S</afterStop>
            </rest>
            <instruction>

                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>freestyle</standardKick>
                            </kicking>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>easy</zone>
                            </startIntensity>
                            <stopIntensity>
                                <zone>racePace</zone>
                            </stopIntensity>
                        </intensity>
            </instruction>

            <instruction>

                        <continue>
                            <continueLength>
                                <lengthAsDistance>100</lengthAsDistance>
                            </continueLength>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <drill>
                                        <drillName>6KickDrill</drillName>
                                        <drillStroke>freestyle</drillStroke>
                                    </drill>
                                </stroke>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <drill>
                                        <drillName>fingerTrails</drillName>
                                        <drillStroke>freestyle</drillStroke>
                                    </drill>
                                </stroke>
                            </instruction>
                        </continue>
            </instruction>

            <instruction>

                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>freestyle</standardKick>
                            </kicking>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>easy</zone>
                            </startIntensity>
                            <stopIntensity>
                                <zone>racePace</zone>
                            </stopIntensity>
                        </intensity>
            </instruction>

            <instruction>

                        <continue>
                            <continueLength>
                                <lengthAsDistance>100</lengthAsDistance>
                            </continueLength>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <breath>3</breath>
                                <equipment>pullBuoy</equipment>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <breath>5</breath>
                                <equipment>pullBuoy</equipment>
                            </instruction>
                        </continue>
            </instruction>

        </repetition>
    </instruction>

    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>

            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>

            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <continue>
                            <continueLength>100</continueLength>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <intensity>
                                    <startIntensity>
                                        <zone>easy</zone>
                                    </startIntensity>
                                </intensity>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <intensity>
                                    <startIntensity>
                                        <zone>endurance</zone>
                                    </startIntensity>
                                </intensity>
                            </instruction>
                        </continue>
                    </instruction>
                </repetition>
            </instruction>

            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>individualMedley</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>

            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <equipment>pads</equipment>
                        <equipment>pullBuoy</equipment>
                    </instruction>
                </repetition>
            </instruction>

            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <equipment>pullBuoy</equipment>
                    <equipment>pads</equipment>
                    <instruction>
                        <continue>
                            <continueLength>100</continueLength>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <intensity>
                                    <startIntensity>
                                        <zone>easy</zone>
                                    </startIntensity>
                                </intensity>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <intensity>
                                    <startIntensity>
                                        <zone>endurance</zone>
                                    </startIntensity>
                                </intensity>
                            </instruction>
                        </continue>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>individualMedley</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <continue>
                            <continueLength>100</continueLength>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <intensity>
                                    <startIntensity>
                                        <zone>easy</zone>
                                    </startIntensity>
                                </intensity>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <standardStroke>freestyle</standardStroke>
                                </stroke>
                                <intensity>
                                    <startIntensity>
                                        <zone>endurance</zone>
                                    </startIntensity>
                                </intensity>
                            </instruction>
                        </continue>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedley</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            
        </repetition>
    </instruction>




    <instruction>
        <segmentName>Second set</segmentName>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>notFreestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>125</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <afterStop>PT0M20S</afterStop>
                </rest>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>nr1</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M30S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
                <rest>
                    <sinceStart>PT0M45S</sinceStart>
                </rest>
                <underwater>true</underwater>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M45S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>300</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
</program>

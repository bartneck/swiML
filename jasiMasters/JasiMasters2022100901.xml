<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>This Sunday morning program mixed endurance with sprints. The target duration is 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2022-10-09</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>any</drillStroke>
        </drill></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>25</lengthAsDistance>
                        <rest><sinceStart>PT45S</sinceStart></rest>
                        <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                        <stroke><standardStroke>any</standardStroke></stroke>
                        <equipment>fins</equipment>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <lengthAsDistance>300</lengthAsDistance>
                <rest><afterStop>PT45S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>25</lengthAsDistance>
                        <rest><sinceStart>PT45S</sinceStart></rest>
                        <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <lengthAsDistance>300</lengthAsDistance>
                <rest><afterStop>PT45S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>25</lengthAsDistance>
                        <rest><sinceStart>PT45S</sinceStart></rest>
                        <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                        <stroke><standardStroke>any</standardStroke></stroke>
                        <equipment>pads</equipment>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <lengthAsDistance>300</lengthAsDistance>
                <rest><afterStop>PT45S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Swim down</segmentName></instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>
<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.0/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our sunday morning program. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2024-02-12</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity><zone>easy</zone></startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <rest><afterStop>PT0M10S</afterStop></rest>
            <equipment>fins</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>6KickDrill</drillName>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>123</drillName>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>singleArm</drillName>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <stroke>
                <standardStroke>freestyle</standardStroke>
            </stroke>
            <rest>
                <afterStop>PT1M0S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>400</lengthAsDistance>
                </length>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>400</lengthAsDistance>
                </length>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <stroke>
                <standardStroke>freestyle</standardStroke>
            </stroke>
            <rest>
                <afterStop>PT0M45S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>300</lengthAsDistance>
                </length>
                <intensity>
                    <startIntensity>
                        <zone>endurance</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>300</lengthAsDistance>
                </length>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <stroke>
                <standardStroke>freestyle</standardStroke>
            </stroke>
            <rest>
                <afterStop>PT0M30S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <intensity>
                    <startIntensity>
                        <zone>endurance</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>200</lengthAsDistance>
                </length>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <stroke>
                <standardStroke>freestyle</standardStroke>
            </stroke>
            <rest>
                <afterStop>PT0M15S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <intensity>
                    <startIntensity>
                        <zone>endurance</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <equipment>fins</equipment>
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>butterfly</standardStroke>
                        </stroke>
                        <underwater>true</underwater>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>butterfly</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
</program>
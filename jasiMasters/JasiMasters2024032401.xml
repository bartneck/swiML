<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.0/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning program. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2024-03-24</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <continue>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <drill>
                                        <drillName>singleArm</drillName>
                                        <drillStroke>butterfly</drillStroke>
                                    </drill>
                                </stroke>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>50</lengthAsDistance>
                                </length>
                                <stroke>
                                    <kicking>
                                        <orientation>back</orientation>
                                        <legMovement>dolpine</legMovement>
                                    </kicking>
                                </stroke>
                            </instruction>
                        </continue>
                    </instruction>
                </repetition>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <breath>5</breath>
        <equipment>pullBuoy</equipment>
    </instruction>
<instruction>
    <segmentName>First set</segmentName>
</instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <rest>
                <afterStop>PT0M20S</afterStop>
            </rest>
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedleyOrder</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>individualMedleyOrder</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
<!--            <repetitionCount>4</repetitionCount>-->
            <simplify>true</simplify>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <stroke>
                        <standardStroke>freestyle</standardStroke>
                    </stroke>
                    <equipment>pullBuoy</equipment>
                    <equipment>pads</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <rest>
                            <afterStop>PT0M20S</afterStop>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>nr1</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M20S</afterStop>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <equipment>fins</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>any</standardKick>
                            </kicking>
                        </stroke>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M20S</afterStop>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M20S</afterStop>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Second set</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <kicking>
                <standardKick>any</standardKick>
            </kicking>
        </stroke>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>individualMedleyOrder</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>individualMedleyOrder</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOrder</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
</program>

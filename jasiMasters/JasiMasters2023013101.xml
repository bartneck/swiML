<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning program in the sun. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2023-01-31</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>backstroke</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
                <drillName>6KickDrill</drillName>
                <drillStroke>freestyle</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <orientation>side</orientation>
                <legMovement>flutter</legMovement>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT30S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <breath>5</breath>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>individualMedley</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><dynamicAcross>false</dynamicAcross>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
            <stopIntensity>
                <zone>racePace</zone>
            </stopIntensity>
        </intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>150</lengthAsDistance>
                <rest><sinceStart>PT2M30S</sinceStart></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><sinceStart>PT70S</sinceStart></rest>
        <stroke><standardStroke>butterfly</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><sinceStart>PT70S</sinceStart></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>150</lengthAsDistance>
                <rest><sinceStart>PT2M30S</sinceStart></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><sinceStart>PT70S</sinceStart></rest>
        <stroke><standardStroke>breaststroke</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><sinceStart>PT70S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><sinceStart>PT1M35S</sinceStart></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT60S</sinceStart></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>

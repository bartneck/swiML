<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Sunday program that mixed endurance with sprints. The target duration is 60 minutes.</programDescription>
    <creationDate>2022-10-02</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT65S</sinceStart></rest>
                <intensity><staticIntensity><precentageEffort>70</precentageEffort></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT75S</sinceStart></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>technic</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT75S</sinceStart></rest>
                <intensity><staticIntensity><precentageEffort>80</precentageEffort></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>backstroke</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT64S</sinceStart></rest>
                <intensity><staticIntensity><precentageEffort>70</precentageEffort></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT90S</sinceStart></rest>
                <intensity><staticIntensity><precentageEffort>90</precentageEffort></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Second set and warm down</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT40S</sinceStart></rest>
                <stroke><drill>
                    <drillName>technic</drillName>
                    <drillStroke>individualMedleyOrder</drillStroke>
                </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
</program>
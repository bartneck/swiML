<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our first 2023 training session in the outdoor pool. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2023-01-15</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>easy</zone>
                    </staticIntensity></intensity>
                <stroke><kicking>
                        <orientation>side</orientation>
                        <legMovement>flutter</legMovement>
                    </kicking>
                </stroke>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>6KickDrill</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>fingerTrails</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
                <breath>5</breath>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>fins</equipment>
                <instructionDescription>first 15m fast</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>individualMedley</drillStroke>
        </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>breaststroke</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
        <stroke><kicking><standardKick>backstroke</standardKick></kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>butterfly</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><standardStroke>notFreestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>150</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>fins</equipment>
                <equipment>pads</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT70S</afterStop></rest>
                <intensity><dynamicAcross>false</dynamicAcross>
                <startIntensity><zone>easy</zone></startIntensity>
                    <stopIntensity><zone>racePace</zone></stopIntensity>
                </intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT70S</afterStop></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
</program>

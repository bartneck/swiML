<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Sunday morning program targeted at 60 minutes. The coach was Matt Nash</programDescription>
    <creationDate>2024-04-07</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <instruction>
                <continue>
                    <continueLength>300</continueLength>
                    <equipment>fins</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>freestyle</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <orientation>back</orientation>
                                <legMovement>flutter</legMovement>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>butterfly</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>300</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <continue>
                    <continueLength>300</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>individualMedley</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>individualMedley</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedley</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <rest>
                <sinceStart>PT1M0S</sinceStart>
            </rest>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>technic</drillName>
                                <drillStroke>freestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Second set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>300</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <afterStop>PT1M0S</afterStop>
                </rest>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <rest>
                <afterStop>PT0M15S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>individualMedleyOrder</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>individualMedleyOrder</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOrder</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Third set</segmentName>
    </instruction>
    
    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </continue>
    </instruction>
    

    
</program>
<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
   <title>Jasi Masters</title>
   <author>
      <firstName>Christoph</firstName>
      <lastName>Bartneck</lastName>
      <email>christoph@bartneck.de</email>
   </author>
   <programDescription>A sunday program focusing on kick with fins and sprints.</programDescription>
   <creationDate>2022-09-18</creationDate>
   <poolLength>25</poolLength>
   <lengthUnit>meters</lengthUnit>
   <hideIntro>false</hideIntro>
   <instruction>
      <segmentName>Warm Up</segmentName>
   </instruction>
   <instruction>
      <length><lengthAsDistance>400</lengthAsDistance></length>
      <stroke><standardStroke>any</standardStroke></stroke>
      <intensity><startIntensity>
            <zone>easy</zone>
         </startIntensity></intensity>
   </instruction>
   <instruction>
      <length><lengthAsDistance>200</lengthAsDistance></length>
      <stroke><kicking>
            <standardKick>backstroke</standardKick>
         </kicking></stroke>
      <equipment>fins</equipment>
   </instruction>


   <instruction>
      <continue>
         <simplify>true</simplify>
         <equipment>fins</equipment>
         <instruction>
            <length><lengthAsDistance>50</lengthAsDistance></length>
            <stroke><drill>
                  <drillName>6KickDrill</drillName>
                  <drillStroke>freestyle</drillStroke>
               </drill></stroke>
         </instruction>
         <instruction>
            <length><lengthAsDistance>50</lengthAsDistance></length>
            <stroke><drill>
                  <drillName>123</drillName>
                  <drillStroke>freestyle</drillStroke>
               </drill></stroke>
            <rest><afterStop>PT0S</afterStop></rest>
            <equipment>fins</equipment>
         </instruction>
         <instruction>
            <length><lengthAsDistance>50</lengthAsDistance></length>
            <stroke><drill>
                  <drillName>fingerTrails</drillName>
                  <drillStroke>freestyle</drillStroke>
               </drill></stroke>
            <rest><afterStop>PT0S</afterStop></rest>
            <equipment>fins</equipment>
         </instruction>
      </continue>
   </instruction>

   <instruction>
      <length><lengthAsDistance>50</lengthAsDistance></length>
      <stroke><standardStroke>freestyle</standardStroke></stroke>
      <breath>5</breath>
      <equipment>fins</equipment>
   </instruction>

   <instruction>
      <continue>
         <instruction>
            <length><lengthAsDistance>100</lengthAsDistance></length>
            <stroke><kicking>
                  <standardKick>any</standardKick>
               </kicking></stroke>
         </instruction>

         <instruction>
            <length><lengthAsDistance>75</lengthAsDistance></length>
            <stroke><drill>
                  <drillName>any</drillName>
                  <drillStroke>any</drillStroke>
               </drill></stroke>
         </instruction>
         <instruction>
            <length><lengthAsDistance>25</lengthAsDistance></length>
            <stroke><standardStroke>any</standardStroke></stroke>
         </instruction>
      </continue>
   </instruction>

   <instruction>
      <segmentName>Main Program</segmentName>
   </instruction>
   <instruction>
      <repetition>
         <repetitionCount>2</repetitionCount>
         <instruction>
            <repetition>
               <repetitionCount>3</repetitionCount>
               <rest><afterStop>PT1M45S</afterStop></rest>
               <instruction>
                  <continue>
                     <instruction>
                        <length><lengthAsDistance>50</lengthAsDistance></length>
                        <stroke><drill>
                              <drillName>technic</drillName>
                              <drillStroke>freestyle</drillStroke>
                           </drill></stroke>
                     </instruction>
                     <instruction>
                        <length><lengthAsDistance>50</lengthAsDistance></length>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                     </instruction>
                  </continue>
               </instruction>
            </repetition>
         </instruction>
         <instruction>
            <repetition>
               <repetitionCount>2</repetitionCount>
               <instruction>
                  <length><lengthAsDistance>75</lengthAsDistance></length>
                  <stroke><standardStroke>freestyle</standardStroke></stroke>
                  <rest><afterStop>PT45S</afterStop></rest>
                  <intensity><startIntensity>
                        <zone>endurance</zone>
                     </startIntensity></intensity>
               </instruction>
            </repetition>
         </instruction>

         <instruction>
            <repetition>
               <repetitionCount>3</repetitionCount>
               <instruction>
                  <length><lengthAsDistance>25</lengthAsDistance></length>
                  <stroke><kicking>
                        <standardKick>any</standardKick>
                     </kicking></stroke>
                  <rest><sinceStart>PT45S</sinceStart></rest>
               </instruction>
               <instruction>
                  <length><lengthAsDistance>25</lengthAsDistance></length>
                  <stroke><standardStroke>freestyle</standardStroke></stroke>
                  <rest><sinceStart>PT45S</sinceStart></rest>
                  <intensity><startIntensity>
                        <zone>max</zone>
                     </startIntensity>
                  </intensity>
               </instruction>
            </repetition>
         </instruction>

         <instruction>
            <length><lengthAsDistance>50</lengthAsDistance></length>
            <stroke><standardStroke>any</standardStroke></stroke>
            <rest><afterStop>PT30S</afterStop></rest>
            <intensity><startIntensity>
                  <zone>max</zone>
               </startIntensity></intensity>
         </instruction>
         <instruction>
            <length><lengthAsDistance>200</lengthAsDistance></length>
            <stroke><standardStroke>any</standardStroke></stroke>
            <intensity><startIntensity>
                  <zone>easy</zone>
               </startIntensity>
            </intensity>
         </instruction>
      </repetition>
   </instruction>
</program>

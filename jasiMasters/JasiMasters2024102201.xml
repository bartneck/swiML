<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Tuesday evening program targeted at 60 minutes. The coach was Matt Nash</programDescription>
    <creationDate>2024-10-22</creationDate>
    
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <continue>
            <rest>
                <afterStop>PT0M15S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <orientation>side</orientation>
                        <legMovement>flutter</legMovement>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>backstroke</standardKick>
                    </kicking>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <continue>
            <rest>
                <afterStop>PT0M15S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>6KickDrill</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>6KickDrill</drillName>
                        <drillStroke>backstroke</drillStroke>
                    </drill>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <continue>
            <rest>
                <afterStop>PT0M15S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>123</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>123</drillName>
                        <drillStroke>backstroke</drillStroke>
                    </drill>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    
    <instruction>
        <continue>
            <rest>
                <afterStop>PT0M15S</afterStop>
            </rest>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <afterStop>PT0M20S</afterStop>
                    </rest>
                    <equipment>pads</equipment>
                    <equipment>pullBuoy</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>breaststroke</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>technic</drillName>
                                <drillStroke>freestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT1M0S</sinceStart>
                    </rest>
                    <equipment>fins</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>butterfly</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
                <excludeAlign>true</excludeAlign>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>butterfly</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>75</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>breaststroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>breaststroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>75</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>butterfly</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <!-- first -->
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT1M5S</sinceStart>
                    </rest>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>nr1</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>70</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>   
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <length>
            <lengthAsDistance>150</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M30S</afterStop>
        </rest>
    </instruction>
    
    
    <!-- second -->
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT1M15S</sinceStart>
                    </rest>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>nr1</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>80</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>   
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <length>
            <lengthAsDistance>150</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M30S</afterStop>
        </rest>
    </instruction>
    
    
    <!-- third-->
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT1M25S</sinceStart>
                    </rest>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>nr1</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>90</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>   
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <length>
            <lengthAsDistance>150</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M30S</afterStop>
        </rest>
    </instruction>
</program>
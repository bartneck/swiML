<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program in the sun. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2023-02-07</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><kicking>
                <standardKick>freestyle</standardKick>
            </kicking></stroke>
        <equipment>fins</equipment>
        <equipment>pads</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>freestyle</drillStroke>
            </drill></stroke>
        <equipment>fins</equipment>
        <equipment>pads</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>fins</equipment>
        <equipment>pads</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><kicking>
                <standardKick>notFreestyle</standardKick>
            </kicking></stroke>
        <equipment>fins</equipment>
        <equipment>pads</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>notFreestyle</drillStroke>
            </drill></stroke>
        <equipment>fins</equipment>
        <equipment>pads</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
        <equipment>fins</equipment>
        <equipment>pads</equipment>
    </instruction>

    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><sinceStart>PT1M40S</sinceStart></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <rest><sinceStart>PT3M30S</sinceStart></rest>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><sinceStart>PT1M45S</sinceStart></rest>
        <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT1M</sinceStart></rest>
                <intensity><staticIntensity><zone>threshold</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>any</drillStroke>
        </drill></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><sinceStart>PT1M45S</sinceStart></rest>
                <intensity><staticIntensity><zone>threshold</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>any</drillStroke>
        </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><dynamicAcross>false</dynamicAcross>
        <startIntensity><zone>easy</zone></startIntensity>
        <stopIntensity><zone>racePace</zone></stopIntensity>
        </intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>

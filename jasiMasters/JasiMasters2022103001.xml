<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning session focusing on speed variations. The program was provided by Matt Nash. The target duration was 60 minutes.</programDescription>
    <creationDate>2022-10-30</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT60S</afterStop></rest>
                <stroke><drill>
                        <drillName>technic</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking>
                        <standardKick>any</standardKick>
                    </kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><drill>
                        <drillName>any</drillName>
                        <drillStroke>any</drillStroke>
                    </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT40S</afterStop></rest>
        <stroke><drill>
                <drillName>technic</drillName>
                <drillStroke>freestyle</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT40S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>racePace</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT40S</afterStop></rest>
        <stroke><drill>
                <drillName>technic</drillName>
                <drillStroke>nr1</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT40S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>racePace</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>nr1</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT40S</afterStop></rest>
        <stroke><drill>
                <drillName>technic</drillName>
                <drillStroke>any</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT40S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>racePace</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>75</lengthAsDistance>
                        <rest><sinceStart>PT90S</sinceStart></rest>
                        <intensity><dynamicAcross>true</dynamicAcross>
                            <startIntensity><precentageEffort>50</precentageEffort></startIntensity>
                            <stopIntensity><precentageEffort>70</precentageEffort></stopIntensity>
                        </intensity>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <lengthAsDistance>50</lengthAsDistance>
                        <rest><sinceStart>PT60S</sinceStart></rest>
                        <intensity><staticIntensity><precentageEffort>80</precentageEffort></staticIntensity></intensity>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT60S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><drill>
            <drillName>scull</drillName>
            <drillStroke>breaststroke</drillStroke>
        </drill></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
        <equipment>fins</equipment>
        <instructionDescription>first 10m fast</instructionDescription>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>breaststroke</standardStroke></stroke>
        <equipment>fins</equipment>
        <instructionDescription>first 10m fast</instructionDescription>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>fins</equipment>
        <instructionDescription>first 10m fast</instructionDescription>
    </instruction>
    <instruction><segmentName>Swim down</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>

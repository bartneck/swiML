<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>This Saturday morning program focused on endurance. The target duration is 60 minutes.</programDescription>
    <creationDate>2022-10-08</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>backstroke</drillStroke>
                </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>300</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>150</lengthAsDistance>
                        <rest><afterStop>PT20S</afterStop></rest>
                        <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                        <equipment>pads</equipment>
                        <equipment>pullBuoy</equipment>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>17 times 75</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><repetition>
        <repetitionCount>5</repetitionCount>
        <instruction>
            <lengthAsDistance>75</lengthAsDistance>
            <rest><afterStop>PT15S</afterStop></rest>
            <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
            <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
        </instruction>
    </repetition></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>225</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>
<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Sunday morning program targeted at 60 minutes.</programDescription>
    <creationDate>2024-01-19</creationDate>
    
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <continue>
            <equipment>fins</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>freestyle</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>backstroke</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        
                        <drillName>6KickDrill</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>6KickDrill</drillName>
                        <drillStroke>backstroke</drillStroke>
                    </drill>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <drill>
                <drillName>any</drillName>
                <drillStroke>individualMedley</drillStroke>
            </drill>
        </stroke>
    </instruction>
    
    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>individualMedley</standardStroke>
        </stroke>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <intensity>
                <startIntensity>
                    <zone>easy</zone>
                </startIntensity>
                <stopIntensity>
                    <zone>racePace</zone>
                </stopIntensity>
            </intensity>
            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT2M30S</sinceStart>
                </rest>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>butterfly</standardStroke>
        </stroke>
        <rest>
            <sinceStart>PT1M10S</sinceStart>
        </rest>
    </instruction>
    <instruction>
        <length>
            <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>backstroke</standardStroke>
        </stroke>
        <rest>
            <sinceStart>PT1M10S</sinceStart>
        </rest>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <intensity>
                <startIntensity>
                    <zone>easy</zone>
                </startIntensity>
                <stopIntensity>
                    <zone>racePace</zone>
                </stopIntensity>
            </intensity>
            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT2M30S</sinceStart>
                </rest>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>breaststroke</standardStroke>
        </stroke>
        <rest>
            <sinceStart>PT1M10S</sinceStart>
        </rest>
    </instruction>
    <instruction>
        <length>
            <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <sinceStart>PT1M10S</sinceStart>
        </rest>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT1M45S</sinceStart>
                    </rest>
                    <instruction>
                        <length>
                            <lengthAsDistance>75</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>nr1</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>easy</zone>
                            </startIntensity>
                            <stopIntensity>
                                <zone>racePace</zone>
                            </stopIntensity>
                        </intensity>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>nr1</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M30S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
</program>

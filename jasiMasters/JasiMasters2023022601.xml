<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning program in inside pool due to an event. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2023-02-26</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT10S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>any</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT10S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT10S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>any</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><afterStop>PT10S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>individualMedley</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
                <drillName>any</drillName>
                <drillStroke>individualMedley</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT30S</afterStop></rest>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>

    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <stroke><drill>
                <drillName>technic</drillName>
                <drillStroke>freestyle</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <intensity><staticIntensity>
                <zone>max</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <stroke><drill>
                <drillName>technic</drillName>
                <drillStroke>nr1</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <rest><sinceStart>PT45S</sinceStart></rest>
        <intensity><staticIntensity>
                <zone>max</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>nr1</standardStroke></stroke>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><sinceStart>PT3M10S</sinceStart></rest>
                <intensity><dynamicAcross>true</dynamicAcross>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
    </instruction>
    
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
        <instructionDescription>5 sec glide at 12.5m</instructionDescription>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
            <drillName>fingerTrails</drillName>
            <drillStroke>freestyle</drillStroke>
        </drill></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
        <instructionDescription>5 sec glide at 12.5m</instructionDescription>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>10</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>notFreestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceLastRest>PT1M</sinceLastRest></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <instructionDescription>5th and 10th fast</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
</program>

<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>This 60 minutes Tuesday evening program has a littl bit of everything. The coach was Matt Nash.</programDescription>
    <creationDate>2022-09-27</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT60S</sinceStart></rest>
                <intensity>
                    <dynamicAcross>true</dynamicAcross>
                    <startIntensity><zone>easy</zone></startIntensity>
                    <stopIntensity><zone>max</zone></stopIntensity>
                </intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
                <underwater>true</underwater>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>125</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
                <instructionDescription>Each stroke 50m once.</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT45S</sinceStart></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>individualMedleyOrder</standardStroke></stroke>
                <equipment>pads</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT60S</sinceStart></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>individualMedleyOrder</standardKick></kicking></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>125</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <stroke><drill>
                    <drillName>technic</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking><standardKick>nr1</standardKick></kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT10S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Swim down</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>
<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Sunday morning program targeted at 60 minutes.</programDescription>
    <creationDate>2024-12-01</creationDate>
    
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <continue>
                    <equipment>fins</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <orientation>side</orientation>
                                <legMovement>dolphin</legMovement>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>singleArm</drillName>
                                <drillStroke>butterfly</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <continue>
                    <equipment>fins</equipment>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <orientation>side</orientation>
                                <legMovement>flutter</legMovement>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>6KickDrill</drillName>
                                <drillStroke>freestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>individualMedley</standardStroke>
        </stroke>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>


    <instruction>
        <length>
            <lengthAsDistance>150</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <afterStop>PT0M20S</afterStop>
        </rest>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <intensity>
                <startIntensity>
                    <zone>easy</zone>
                </startIntensity>
                <stopIntensity>
                    <zone>racePace</zone>
                </stopIntensity>
            </intensity>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <kicking>
                <standardKick>any</standardKick>
            </kicking>
        </stroke>
    </instruction>
    
    
    <instruction>
        <segmentName>Second set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>250</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <inOut>8</inOut>
                </rest>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>nr1</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>butterfly</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>75</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>backstroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>breaststroke</standardStroke>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>
</program>



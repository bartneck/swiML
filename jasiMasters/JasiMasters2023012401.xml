<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program in the inside pool. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2023-01-24</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
            <zone>easy</zone>
        </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking><standardKick>freestyle</standardKick></kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>freestyle</drillStroke>
        </drill></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking><standardKick>notFreestyle</standardKick></kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>notFreestyle</drillStroke>
        </drill></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><drill>
                    <drillName>scull</drillName>
                    <drillStroke>breaststroke</drillStroke>
                </drill></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>15</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
        <underwater>true</underwater>
    </instruction>
    <instruction>
        <lengthAsDistance>60</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><drill>
            <drillName>technic</drillName>
            <drillStroke>any</drillStroke>
        </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <stroke><standardStroke>any</standardStroke></stroke>
        <underwater>true</underwater>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><sinceStart>PT1M35S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <rest><sinceStart>PT3M15S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT1M</afterStop></rest>
        <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><sinceStart>PT1M35S</sinceStart></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>300</lengthAsDistance>
        <rest><afterStop>PT45S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT60S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><sinceStart>PT2M</sinceStart></rest>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT1M</sinceStart></rest>
                <stroke><standardStroke>individualMedleyOrder</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT1M</afterStop></rest>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><sinceStart>PT1M</sinceStart></rest>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><sinceStart>PT2M</sinceStart></rest>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><sinceStart>PT1M</sinceStart></rest>
        <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
    </instruction>
    <instruction><segmentName>Swim down</segmentName></instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>
                                <drillName>any</drillName>
                                <drillStroke>any</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>max</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </continue>
            </instruction>
            
            <instruction>
                <continue>
                    <continueLength>75</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>max</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </continue>
            </instruction>
            
            <instruction>
                <continue>
                    <continueLength>75</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>any</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>any</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>max</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <segmentName>First set</segmentName>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instructionDescription>Second with fins</instructionDescription>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT0M60S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>technic</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
                <rest>
                    <sinceStart>PT1M45S</sinceStart>
                </rest>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>

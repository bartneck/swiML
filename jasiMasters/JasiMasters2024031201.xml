<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.0/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2024-03-12</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <repetition>
            <simplify>true</simplify>
            <instruction>
                <continue>
                    <continueLength>150</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>freestyle</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>freestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
            
            <instruction>
                <continue>
                    <continueLength>150</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>notFreestyle</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>notFreestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>

            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <continue>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <kicking>
                                        <standardKick>freestyle</standardKick>
                                    </kicking>
                                </stroke>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <kicking>
                                        <standardKick>backstroke</standardKick>
                                    </kicking>
                                </stroke>
                            </instruction>
                        </continue>
                    </instruction>
                </repetition>
            </instruction>

        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <simplify>true</simplify>
            <equipment>fins</equipment>
            
            <instruction>
                <continue>
                    <continueLength>75</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>any</standardKick>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <standardKick>any</standardKick>
                            </kicking>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>max</zone>
                            </startIntensity>
                        </intensity>
                    </instruction>
                </continue>
            </instruction>
            
            <instruction>
                <continue>
                    <continueLength>75</continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>

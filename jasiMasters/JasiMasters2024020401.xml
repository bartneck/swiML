<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.0/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning program. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2024-02-04</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity><zone>easy</zone></startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <simplify>true</simplify>
            <equipment>fins</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <kicking><standardKick>any</standardKick></kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
                <equipment>pads</equipment>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <simplify>true</simplify>
            <stroke><standardStroke>freestyle</standardStroke></stroke>
            <equipment>pads</equipment>
            <equipment>pullBuoy</equipment>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <rest><sinceStart>PT1M50S</sinceStart></rest>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <rest><sinceStart>PT1M45S</sinceStart></rest>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <rest><sinceStart>PT1M40S</sinceStart></rest>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <rest><sinceStart>PT1M35S</sinceStart></rest>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity><zone>easy</zone></startIntensity>
        </intensity>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <simplify>true</simplify>
            <instruction>
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedleyOrder</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT1M0S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
             <instruction>   
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <intensity>
                        <startIntensity><zone>easy</zone></startIntensity>
                        <stopIntensity><zone>racePace</zone></stopIntensity>
                    </intensity>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT0M50S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>
             </instruction>
                <instruction>
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>reverseIndividualMedleyOrder</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT1M0S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>
                </instruction>
                <instruction>
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <intensity>
                        <startIntensity><zone>easy</zone></startIntensity>
                        <stopIntensity><zone>racePace</zone></stopIntensity>
                    </intensity>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>nr1</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT0M50S</sinceStart>
                        </rest>
                        <equipment>fins</equipment>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke><standardStroke>any</standardStroke></stroke>
        <intensity>
            <startIntensity><zone>easy</zone></startIntensity>
        </intensity>
    </instruction>
    
</program>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program targeted at one hour. The coach was Matt.</programDescription>
    <creationDate>2024-02-25</creationDate>
    <pool>
        <poolLength>25</poolLength>
        <lengthUnit>meters</lengthUnit>
    </pool>
    <layoutWidth>60</layoutWidth>
    
    <instruction><segmentName>WarmUp</segmentName></instruction>
    <instruction><length><lengthAsDistance>400</lengthAsDistance></length><stroke><standardStroke>any</standardStroke></stroke><intensity><startIntensity>
                <zone>easy</zone>
            </startIntensity></intensity></instruction>
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <rest><afterStop>PT0M15S</afterStop></rest>
            <equipment>fins</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
            </instruction>

            <instruction>
                <continue>
                    <continueLength>
                        <lengthAsDistance>150</lengthAsDistance>
                    </continueLength> 
                    <instruction>
                        <repetition>
                            <repetitionCount>3</repetitionCount>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <drill>
                                        <drillName>6KickDrill</drillName>
                                        <drillStroke>freestyle</drillStroke>
                                    </drill>
                                </stroke>
                            </instruction>
                            <instruction>
                                <length>
                                    <lengthAsDistance>25</lengthAsDistance>
                                </length>
                                <stroke>
                                    <drill>
                                        <drillName>singleArm</drillName>
                                        <drillStroke>butterfly</drillStroke>
                                    </drill>
                                </stroke>
                            </instruction>
                        </repetition>
                    </instruction>
                    
                    
                </continue>
            </instruction>

            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke><standardStroke>any</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
</program>

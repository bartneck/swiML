<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters (Unofficial)</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>We adapted a program from "The Waterproof Swimmer" book. It was targeted at the faster swimmers of the club. Estimated duration was 90 minutes.</programDescription>
    <creationDate>2022-09-24</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><inOut>3</inOut></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><inOut>3</inOut></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><inOut>3</inOut></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><inOut>3</inOut></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>125</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
                <instructionDescription>Each stroke 50m once.</instructionDescription>
            </instruction>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><sinceStart>PT1M45S</sinceStart></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>notFreestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <segmentName>Second set and warm down</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>freestyle</standardKick></kicking></stroke>
                <underwater>true</underwater>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceLastRest>PT55S</sinceLastRest></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity><zone>racePace</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceLastRest>PT55S</sinceLastRest></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>300</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
</program>
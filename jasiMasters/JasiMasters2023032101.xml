<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program in the sun. The target duration was 60 minutes. The coach was Matt Nash.</programDescription>
    <creationDate>2023-03-21</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>freestyle</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><kicking>
                <standardKick>backstroke</standardKick>
            </kicking></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>150</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><standardStroke>individualMedleyOverlap</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT15S</afterStop></rest>
        <stroke><standardStroke>breaststroke</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT1M</sinceStart></rest>
                <intensity><dynamicAcross>true</dynamicAcross>
                    <startIntensity>
                        <precentageEffort>80</precentageEffort>
                    </startIntensity>
                    <stopIntensity>
                        <precentageEffort>100</precentageEffort>
                    </stopIntensity>
                </intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT1M</sinceStart></rest>
                <stroke><drill>
                        <drillName>technic</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill></stroke>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>breaststroke</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT0S</afterStop></rest>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>max</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>individualMedleyOverlap</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceLastRest>PT70S</sinceLastRest></rest>
                <intensity><staticIntensity>
                        <zone>max</zone>
                    </staticIntensity></intensity>
                <stroke><drill>
                        <drillName>technic</drillName>
                        <drillStroke>individualMedleyOverlap</drillStroke>
                    </drill></stroke>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking>
                        <standardKick>freestyle</standardKick>
                    </kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><kicking>
                        <standardKick>any</standardKick>
                    </kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT75S</sinceStart></rest>
                <intensity><dynamicAcross>false</dynamicAcross>
                    <startIntensity>
                        <precentageEffort>80</precentageEffort>
                    </startIntensity>
                    <stopIntensity>
                        <precentageEffort>100</precentageEffort>
                    </stopIntensity>
                </intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT75S</sinceStart></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>individualMedleyOverlap</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
</program>

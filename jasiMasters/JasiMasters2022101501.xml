<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Saturday morning session. The program was provided by Marco. The target duration was 90 minutes.</programDescription>
    <creationDate>2022-10-15</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <intensity><staticIntensity>
                        <zone>easy</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>50</lengthAsDistance>
                        <intensity><staticIntensity>
                                <zone>easy</zone>
                            </staticIntensity></intensity>
                        <stroke><standardStroke>individualMedleyOverlap</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>6</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <stroke><drill>
                        <drillName>any</drillName>
                        <drillStroke>any</drillStroke>
                    </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>6</repetitionCount>
                    <instruction>
                        <lengthAsDistance>75</lengthAsDistance>
                        <rest><sinceStart>PT1M25S</sinceStart></rest>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>6</repetitionCount>
                    <instruction>
                        <lengthAsDistance>75</lengthAsDistance>
                        <rest><sinceStart>PT1M20S</sinceStart></rest>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <stroke><standardStroke>any</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><sinceStart>PT55S</sinceStart></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
            
        </repetition>
    </instruction>
</program>

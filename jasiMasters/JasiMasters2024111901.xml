<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>Our Tuesday evening program targeted at 60 minutes. The coach was Matt Nash</programDescription>
    <creationDate>2024-11-10</creationDate>

    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>

    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>400</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>freestyle</standardKick>
                    </kicking>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>any</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>

    <instruction>
        <continue>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </continue>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <breath>5</breath>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>First set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <intensity>
                        <startIntensity>
                            <zone>easy</zone>
                        </startIntensity>
                        <stopIntensity>
                            <zone>endurance</zone>
                        </stopIntensity>
                    </intensity>
                    <instruction>
                        <length>
                            <lengthAsDistance>75</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT1M20S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            
            <!-- second -->
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>technic</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
                <rest>
                    <sinceStart>PT1M5S</sinceStart>
                </rest>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>freestyle</standardKick>
                    </kicking>
                </stroke>
                <rest>
                    <sinceStart>PT1M5S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>technic</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
                <rest>
                    <sinceStart>PT1M5S</sinceStart>
                </rest>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M5S</sinceStart>
                </rest>
                <equipment>pullBuoy</equipment>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillName>technic</drillName>
                        <drillStroke>freestyle</drillStroke>
                    </drill>
                </stroke>
                <rest>
                    <sinceStart>PT1M5S</sinceStart>
                </rest>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M5S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>racePace</zone>
                    </startIntensity>
                </intensity>
            </instruction>
            
            <!-- third -->
            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>any</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Second set</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT2M0S</sinceStart>
                    </rest>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedleyOrder</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>70</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>75</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>technic</drillName>
                                <drillStroke>freestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <continue>
                    <rest>
                        <sinceStart>PT1M15S</sinceStart>
                    </rest>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedleyOrder</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <percentageEffort>80</percentageEffort>
                            </startIntensity>
                        </intensity>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>25</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>technic</drillName>
                                <drillStroke>freestyle</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOrder</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M0S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <percentageEffort>100</percentageEffort>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <segmentName>Warm down</segmentName>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>150</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>
</program>

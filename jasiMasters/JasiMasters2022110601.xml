<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Sunday morning session in Kaikoura. The target duration was 90 minutes.</programDescription>
    <creationDate>2022-11-06</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>300</lengthAsDistance>
        <rest><afterStop>PT30S</afterStop></rest>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT10S</afterStop></rest>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><drill>
            <drillName>any</drillName>
            <drillStroke>any</drillStroke>
        </drill></stroke>
    </instruction>
    <instruction><segmentName>First pyramid set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>max</zone></staticIntensity></intensity>
                <stroke><standardStroke>butterfly</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT25S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>150</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>individualMedley</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT25S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>any</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>6</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><drill>
                    <drillName>6KickDrill</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <instructionDescription>Long streamline push off</instructionDescription>
            </instruction>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <instructionDescription>Long strokes</instructionDescription>
            </instruction>
        </repetition>
    </instruction>
</program>
        <stroke><kicking>
                <standardKick>backstroke</standardKick>
            </kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><kicking>
                <standardKick>butterfly</standardKick>
            </kicking></stroke>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><standardStroke>butterfly</standardStroke></stroke>
        <equipment>fins</equipment>
    </instruction>

    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
        <equipment>fins</equipment>

<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>We swam this program on Queen Elizabeth's Memorial Day in New Zealand. She became queen in 1952 and the program is 5200m. The target duration is 90 minutes.</programDescription>
    <creationDate>2022-09-26</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>400</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <rest><afterStop>PT30S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>150</lengthAsDistance>
        <rest><afterStop>PT25S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT10S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><kicking>
                <standardKick>any</standardKick>
            </kicking></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>50</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction><segmentName>Prime number set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>175</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>easy</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <lengthAsDistance>150</lengthAsDistance>
                <rest><afterStop>PT30S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>5</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>racePace</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>7</repetitionCount>
            <instruction>
                <lengthAsDistance>100</lengthAsDistance>
                <rest><sinceStart>PT1M40S</sinceStart></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <segmentName>The following adds up to 11x75</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><kicking>
                        <standardKick>individualMedleyOrder</standardKick>
                    </kicking></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><drill>
                        <drillName>any</drillName>
                        <drillStroke>individualMedleyOrder</drillStroke>
                    </drill></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>individualMedleyOrder</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>75</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity>
                        <zone>endurance</zone>
                    </staticIntensity></intensity>
                <stroke><standardStroke>individualMedleyOrder</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>75</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>threshold</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>butterfly</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>75</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>threshold</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>backstroke</standardStroke></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>75</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>threshold</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>breaststroke</standardStroke></stroke>
    </instruction>

    <instruction>
        <segmentName>Third set</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>25</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <lengthAsDistance>100</lengthAsDistance>
                        <rest><afterStop>PT20S</afterStop></rest>
                        <intensity><staticIntensity>
                                <zone>endurance</zone>
                            </staticIntensity></intensity>
                        <stroke><standardStroke>individualMedley</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <lengthAsDistance>75</lengthAsDistance>
                        <rest><afterStop>PT20S</afterStop></rest>
                        <intensity><staticIntensity>
                                <zone>endurance</zone>
                            </staticIntensity></intensity>
                        <stroke><standardStroke>freestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <lengthAsDistance>50</lengthAsDistance>
                        <rest><afterStop>PT30S</afterStop></rest>
                        <intensity><staticIntensity>
                                <zone>racePace</zone>
                            </staticIntensity></intensity>
                        <stroke><standardStroke>notFreestyle</standardStroke></stroke>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <segmentName>Swim down</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <rest><afterStop>PT20S</afterStop></rest>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>easy</zone>
            </staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>

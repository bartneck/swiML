                <lengthAsDistance>75</lengthAsDistance>
                <rest><sinceLastRest>PT100S</sinceLastRest></rest>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>Second set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT35S</sinceStart></rest>
                <intensity><staticIntensity>
                        <zone>racePace</zone>
                    </staticIntensity></intensity>
                <stroke><kicking>
                        <standardKick>butterfly</standardKick>
                    </kicking></stroke>
                <underwater>true</underwater>
                <equipment>fins</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><sinceStart>PT35S</sinceStart></rest>
                <intensity><staticIntensity>
                        <zone>racePace</zone>
                    </staticIntensity></intensity>

<?xml version='1.0' encoding='utf-8'?>
<program xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="file:/C:/My%20Documents/GitHub/swiML"
    xsi:schemaLocation="file:/C:/My%20Documents/GitHub/swiML ../../swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program targeted at one hour. The coach was Matt.</programDescription>
    <creationDate>2024-05-17</creationDate>
    <pool>
        <poolLength>25</poolLength>
        <lengthUnit>meters</lengthUnit>
    </pool>
    <programAlign>true</programAlign>
    <instruction>
        <segmentName>repetitions in a simplify</segmentName>
    </instruction>
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <instruction>
                <continue>
                    <continueLength>
                        <lengthAsDistance>600</lengthAsDistance>
                    </continueLength>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>any</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>notFreestyle</standardStroke>
                        </stroke>
                    </instruction>
                </continue>
            </instruction>
            
            <!-- second -->
            <instruction>
                <repetition>
                    <repetitionCount>6</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>100</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedley</standardStroke>
                        </stroke>
                        <rest>
                            <sinceStart>PT1M45S</sinceStart>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
            
            <!-- third -->
            <instruction>
                <repetition>
                    <repetitionCount>4</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>150</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>individualMedleyOrder</drillStroke>
                            </drill>
                        </stroke>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                    </instruction>
                </repetition>
            </instruction>
            
            <!-- fourth -->
            <instruction>
                <repetition>
                    <repetitionCount>3</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                        <equipment>pads</equipment>
                    </instruction>
                </repetition>
            </instruction>
            
            <!-- fourth -->
            <instruction>
                <repetition>
                    <repetitionCount>2</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>300</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <rest>
                            <afterStop>PT0M15S</afterStop>
                        </rest>
                        <equipment>pads</equipment>
                    </instruction>
                </repetition>
            </instruction>
            
            <instruction>
                <length>
                    <lengthAsDistance>600</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
</program>
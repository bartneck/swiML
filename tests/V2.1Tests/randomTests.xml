<?xml version='1.0' encoding='utf-8'?>
<program xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="file:/C:/My%20Documents/GitHub/swiML"
    xsi:schemaLocation="file:/C:/My%20Documents/GitHub/swiML ../../swiML.xsd">
    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
    </author>
    <programDescription>Our Tuesday evening program targeted at one hour. The coach was Matt.</programDescription>
    <creationDate>2024-07-11</creationDate>
   
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    
    <!-- 50 meters in a 25 meter pool can be sub divided into 2x25 -->
    <!-- This should result in 25Fl/25BK and 25BR/25FR-->
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <!-- This should result in 25Fl/25BK and 25BK/25BR and 25BR/25FR-->
    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <!-- 75 meters in a 25 meter pool can be sub divided into 3x25 -->
    <!-- This should result in 25Fl/25BK/25BR and 25BK/25BR/25FR-->
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>75</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
    
    <!-- 150 meters in a 25 meter pool can be sub divided into 3x50> -->
    <!-- This could result in 50Fl/50BK/50BR and 50BK/50BR/50FR-->
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>150</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>individualMedleyOverlap</standardStroke>
                </stroke>
            </instruction>
        </repetition>
    </instruction>
</program>
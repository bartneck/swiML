<?xml version="1.0" encoding="UTF-8"?>
<program xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"  
    xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">

    <title>Jasi Masters</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <programDescription>A sunday program focusing on kick with fins and sprints.</programDescription>
    <creationDate>2022-09-18</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>true</hideIntro>
    <!-- not valid -->
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <simplify>true</simplify>
            <equipment>fins</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>freestyle</standardStroke>
                </stroke>
                <breath>5</breath>
            </instruction>
        </repetition>
    </instruction>
    
    <!-- valid -->
    <instruction>
        <continue>
            <equipment>fins</equipment>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                
            </instruction>
        </continue>
    </instruction>
    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <breath>5</breath>
    </instruction>
</program>
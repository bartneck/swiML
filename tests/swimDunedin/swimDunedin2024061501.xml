<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Swimming Dunedin</title>
    <author>
        <firstName>Lars</firstName>
        <lastName>Humer</lastName>
        <email>larshumer@outlook.com</email>
    </author>
    <programDescription>Practise 01 Week 09
    </programDescription>
    <creationDate>2024-06-17</creationDate>
    <poolLength>50</poolLength>
    <lengthUnit>meters</lengthUnit>

    <instruction>
        <segmentName>Skill warm up</segmentName>
    </instruction>
    
    <instruction>
        <repetition>
            <simplify>true</simplify>
            <rest>
                <sinceStart>PT3M0S</sinceStart>
            </rest>
            <equipment>fins</equipment>
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>backstroke</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>individualMedley</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
            <instruction>
                <repetition>
                    <repetitionCount>1</repetitionCount>
                    <instruction>
                        <length>
                            <lengthAsDistance>200</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                    </instruction>
                </repetition>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>6</repetitionCount>
            <rest>
                <sinceStart>PT1M45S</sinceStart>
            </rest>
            <equipment>fins</equipment>
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>50</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <orientation>back</orientation>
                                <legMovement>dolphin</legMovement>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>30</lengthAsDistance>
                        </length>
                        <stroke>
                            <kicking>
                                <orientation>back</orientation>
                                <legMovement>flutter</legMovement>
                            </kicking>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>20</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>racePace</zone>
                            </startIntensity>
                        </intensity>
                        <breath>0</breath>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <rest>
                <sinceStart>PT1M10S</sinceStart>
            </rest>
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>20</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>freestyle</standardStroke>
                        </stroke>
                        <instructionDescription>SVO</instructionDescription>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>30</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>technic</drillName>
                                <drillStroke>any</drillStroke>
                            </drill>
                        </stroke>
                        <instructionDescription>SVO</instructionDescription>
                    </instruction>
                </continue>
            </instruction>
            
            <instruction>
                <continue>
                    <instruction>
                        <length>
                            <lengthAsDistance>20</lengthAsDistance>
                        </length>
                        <stroke>
                            <drill>
                                <drillName>any</drillName>
                                <drillStroke>any</drillStroke>
                            </drill>
                        </stroke>
                    </instruction>
                    <instruction>
                        <length>
                            <lengthAsDistance>30</lengthAsDistance>
                        </length>
                        <stroke>
                            <standardStroke>any</standardStroke>
                        </stroke>
                        <intensity>
                            <startIntensity>
                                <zone>easy</zone>
                            </startIntensity>
                            <stopIntensity>
                                <zone>max</zone>
                            </stopIntensity>
                        </intensity>
                    </instruction>
                </continue>
            </instruction>
        </repetition>
    </instruction>
    
    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <instructionDescription>O Ch</instructionDescription>
    </instruction>
</program>

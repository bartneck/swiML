<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/bartneck/swiML https://raw.githubusercontent.com/bartneck/swiML/main/swiML.xsd">
    <title>Triathlon Club</title>
    <author>
        <firstName>Graeme</firstName>
        <lastName>Woodward</lastName>
    </author>
    <programDescription></programDescription>
    <creationDate>2022-10-12</creationDate>
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <stroke><kicking>
                    <orientation>side</orientation>
                    <legMovement>flutter</legMovement>
                </kicking></stroke>
                <equipment>pads</equipment>
                <equipment>fins</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <stroke><drill>
                    <drillName>scull</drillName>
                    <drillStroke>freestyle</drillStroke>
                </drill></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <equipment>pullBuoy</equipment>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT0S</afterStop></rest>
                <stroke><drill>
                    <drillName>dogPaddle</drillName>
                    <drillStroke>any</drillStroke>
                </drill></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>25</lengthAsDistance>
                <rest><afterStop>PT15S</afterStop></rest>
                <stroke><standardStroke>any</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <repetition>
            <repetitionCount>4</repetitionCount>
            <instruction>
                <lengthAsDistance>50</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity>
                    <dynamicAcross>true</dynamicAcross>
                    <startIntensity><zone>easy</zone></startIntensity>
                    <stopIntensity><zone>max</zone></stopIntensity>
                </intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
        </repetition>
    </instruction>
    <instruction><segmentName>First set</segmentName></instruction>
    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
            </instruction>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pullBuoy</equipment>
            </instruction>
            <instruction>
                <lengthAsDistance>200</lengthAsDistance>
                <rest><afterStop>PT20S</afterStop></rest>
                <intensity><staticIntensity><zone>endurance</zone></staticIntensity></intensity>
                <stroke><standardStroke>freestyle</standardStroke></stroke>
                <equipment>pads</equipment>
                <equipment>pullBuoy</equipment>
            </instruction>
        </repetition>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <stroke><kicking><standardKick>any</standardKick></kicking></stroke>
    </instruction>
    <instruction><segmentName>Swim down</segmentName></instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <intensity><staticIntensity><zone>easy</zone></staticIntensity></intensity>
        <stroke><standardStroke>any</standardStroke></stroke>
    </instruction>
</program>
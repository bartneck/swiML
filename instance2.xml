<?xml version="1.0" encoding="UTF-8"?>
<program xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="file:/Users/cba56/Dropbox%20(UC%20Enterprise)/swimML/oxygen/swiML.xsd"
    xmlns="https://github.com/bartneck/swiML">

    <!-- This program demonstrates only basic instructions -->
    <!-- Id doesn't have any repetitions or pyramids -->

    <title>Simple Program with only direct instructions</title>
    <author>
        <firstName>Christoph</firstName>
        <lastName>Bartneck</lastName>
        <email>christoph@bartneck.de</email>
    </author>
    <author>
        <firstName>Christine</firstName>
        <lastName>McCurdy</lastName>
    </author>
    <programDescription>A short program to warm up.</programDescription>
    <creationDate>2022-07-04</creationDate>
    <poolLength>25</poolLength>
    <poolLengthUnit>meters</poolLengthUnit>
    <defaultInstructionLengthUnit>meters</defaultInstructionLengthUnit>
    <programLength>800</programLength>
    <hideIntro>true</hideIntro>

    <instruction>
        <segmentName>Warm Up</segmentName>
    </instruction>

    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><afterStop>PT0M20S</afterStop></rest>
        <intensity><staticIntensity>
                <precentageEffort>70</precentageEffort>
            </staticIntensity></intensity>
        <stroke><standardStroke>freestyle</standardStroke></stroke>
        <breath>3</breath>
        <equipment>pads</equipment>
        <equipment>pullBuoy</equipment>
        <instructionDescription>Focus on body position</instructionDescription>
    </instruction>
    <instruction>
        <lengthAsDistance>200</lengthAsDistance>
        <intensity><staticIntensity>
                <zone>endurance</zone>
            </staticIntensity></intensity>
        <stroke><kicking>
                <orientation>front</orientation>
                <legMovement>flutter</legMovement>
            </kicking></stroke>
        <underwater>true</underwater>
        <equipment>fins</equipment>
    </instruction>
    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity>
            <dynamicAcross>false</dynamicAcross>
            <startIntensity>
                <precentageEffort>70</precentageEffort>
            </startIntensity>
            <stopIntensity>
                <precentageEffort>100</precentageEffort>
            </stopIntensity>
        </intensity>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
        <instructionDescription>Focus on underwater phase</instructionDescription>
    </instruction>


    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <rest><sinceStart>PT1M25S</sinceStart></rest>
        <intensity>
            <dynamicAcross>true</dynamicAcross>
            <startIntensity>
                <precentageHeartRate>70</precentageHeartRate>
            </startIntensity>
            <stopIntensity>
                <precentageHeartRate>100</precentageHeartRate>
            </stopIntensity>
        </intensity>
        <stroke><standardStroke>individualMedley</standardStroke></stroke>
        <instructionDescription>Focus on underwater phase</instructionDescription>
    </instruction>

    <instruction>
        <segmentName>Main</segmentName>
    </instruction>

    <instruction>
        <lengthAsDistance>100</lengthAsDistance>
        <intensity><staticIntensity>
                <precentageHeartRate>90</precentageHeartRate>
            </staticIntensity></intensity>
        <stroke><drill>
                <drillNameType>123</drillNameType>
                <drillStroke>freestyle</drillStroke>
            </drill></stroke>
    </instruction>
    <instruction>
        <lengthAsDistance>300</lengthAsDistance>
        <rest><afterStop>PT1M00S</afterStop></rest>
        <stroke><kicking>
                <standardKick>individualMedley</standardKick>
            </kicking></stroke>
    </instruction>

    <instruction>
        <lengthAsTime>PT05M09S</lengthAsTime>
        <stroke><standardStroke>any</standardStroke></stroke>
        <breath>5</breath>
    </instruction>
</program>

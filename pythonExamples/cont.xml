<?xml version='1.0' encoding='utf-8'?>
<program xmlns="https://github.com/bartneck/swiML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://github.com/bartneck/swiML/version/2/2.0 https://raw.githubusercontent.com/bartneck/swiML/main/version/2/2.0/swiML.xsd">
  <title>Continue Examples</title>
  <author>
    <firstName>Callum</firstName>
    <lastName>Lockhart</lastName>
  </author>
  <programDescription>Examples of continues in swiML</programDescription>
  <creationDate>2024-09-23</creationDate>
  <poolLength>25</poolLength>
  <lengthUnit>meters</lengthUnit>
  <programAlign>false</programAlign>
  <numeralSystem>roman</numeralSystem>
  <instruction>
    <continue>
      <continueLength>
        <lengthAsDistance>200</lengthAsDistance>
      </continueLength>
      <instruction>
        <length>
          <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
          <standardStroke>butterfly</standardStroke>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
      <instruction>
        <length>
          <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
          <standardStroke>breaststroke</standardStroke>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
    </continue>
  </instruction>
  <instruction>
    <continue>
      <continueLength>
        <lengthAsDistance>400</lengthAsDistance>
      </continueLength>
      <instruction>
        <length>
          <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
          <drill>
            <drillName>any</drillName>
            <drillStroke>any</drillStroke>
          </drill>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
      <instruction>
        <length>
          <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
          <standardStroke>any</standardStroke>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
    </continue>
  </instruction>
  <instruction>
    <continue>
      <continueLength>
        <lengthAsDistance>400</lengthAsDistance>
      </continueLength>
      <instruction>
        <length>
          <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
          <standardStroke>freestyle</standardStroke>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
      <instruction>
        <length>
          <lengthAsDistance>50</lengthAsDistance>
        </length>
        <stroke>
          <standardStroke>backstroke</standardStroke>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
      <instruction>
        <length>
          <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
          <kicking>
            <standardKick>any</standardKick>
          </kicking>
        </stroke>
        <excludeAlign>false</excludeAlign>
      </instruction>
    </continue>
  </instruction>
</program>
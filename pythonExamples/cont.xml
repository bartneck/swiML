<?xml version="1.0" encoding="UTF-8"?>
<program xmlns="https://github.com/bartneck/swiML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://github.com/bartneck/swiML/version/1/1.1 https://raw.githubusercontent.com/bartneck/swiML/main/version/1/1.1/swiML.xsd">
    <title>Vikings</title>
    <author>
        <firstName>Callum</firstName>
        <lastName>Lockhart</lastName>
        <email>clo78@uclive.ac.nz</email>
    </author>
    <programDescription>Wednesday Afternoon Training</programDescription>
    <creationDate>2024-11-13</creationDate>
    
    <poolLength>25</poolLength>
    <lengthUnit>meters</lengthUnit>
    <hideIntro>false</hideIntro>
    
    <instruction>
        <segmentName>Warm up</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>200</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>freestyle</standardStroke>
        </stroke>
        <rest>
            <sinceStart>PT3M10S</sinceStart>
        </rest>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>50</lengthAsDistance>
                </length>
                <stroke>
                    <drill>
                        <drillStroke>any</drillStroke>
                    </drill>
                </stroke>
                <rest>
                    <sinceStart>PT1M10S</sinceStart>
                </rest>
                <!-- we had specified drills for each stroke e.g. fly was 4k 1stroke or bk was pause, but cant represent this atm-->
            </instruction>
        </repetition>        
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>3</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
                <rest>
                    <sinceStart>PT2M10S</sinceStart>
                </rest>
            </instruction>
        </repetition>        
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>100</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>2</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>100</lengthAsDistance>
                </length>
                <stroke>
                    <kicking>
                        <standardKick>any</standardKick>
                    </kicking>
                </stroke>
                <intensity>
                    <startIntensity>
                        <zone>easy</zone>
                    </startIntensity>
                    <stopIntensity>
                        <zone>racePace</zone>
                    </stopIntensity>
                </intensity>
            </instruction>
        </repetition>        
    </instruction>

    <instruction>
        <segmentName>Main Set</segmentName>
    </instruction>

    <instruction>
        <repetition>
            <repetitionCount>8</repetitionCount>
            <instruction>
                <length>
                    <lengthAsDistance>75</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>nr1</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT1M20S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <percentageEffort>85</percentageEffort>
                    </startIntensity>
                </intensity>
            </instruction>
            <instruction>
                <length>
                    <lengthAsDistance>25</lengthAsDistance>
                </length>
                <stroke>
                    <standardStroke>nr1</standardStroke>
                </stroke>
                <rest>
                    <sinceStart>PT2M0S</sinceStart>
                </rest>
                <intensity>
                    <startIntensity>
                        <zone>max</zone>
                    </startIntensity>
                </intensity>
            </instruction>
        </repetition>
    </instruction>

    <instruction>
        <segmentName>Warm Down</segmentName>
    </instruction>

    <instruction>
        <length>
            <lengthAsDistance>1000</lengthAsDistance>
        </length>
        <stroke>
            <standardStroke>any</standardStroke>
        </stroke>
        <intensity>
            <startIntensity>
                <zone>easy</zone>
            </startIntensity>
        </intensity>
        <equipment>fins</equipment>
    </instruction>
    
</program>